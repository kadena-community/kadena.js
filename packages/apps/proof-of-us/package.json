{
  "name": "@kadena/proof-of-us",
  "version": "0.3.4",
  "private": true,
  "scripts": {
    "build": "pnpm run generate:sdk && next build",
    "create-event": "ts-node -T ./src/scripts/createEvent.ts",
    "dev": "concurrently --kill-others \"npm:next:dev\" \"npm:generate:sdk:watch\"",
    "dev:save": "next dev  --experimental-https",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:src": "prettier . --cache --write",
    "generate:sdk": "cache-sh -i \"{codegen-sdk.yml,./node_modules/@kadena/graph/generated-schema.graphql,src/**/*.graph.ts,src/__generated__/**}\" -- graphql-codegen --config codegen-sdk.yml",
    "generate:sdk:watch": "chokidar \"src/**/*.graph.ts\" -c \"cache-sh -i \"{./node_modules/@kadena/graph/generated-schema.graphql,src/**/*.graph.ts,src/__generated__/**}\" -- graphql-codegen --config codegen-sdk.yml\" --initial",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:src": "eslint src --ext .js,.ts,.jsx,.tsx,.mjs",
    "next:dev": "next dev",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "~3.9.11",
    "@google-analytics/data": "~3.2.2",
    "@graphql-yoga/apollo-link": "~3.3.0",
    "@kadena/client": "workspace:*",
    "@kadena/graph": "workspace:*",
    "@kadena/pactjs": "workspace:*",
    "@kadena/react-icons": "workspace:*",
    "@kadena/react-ui": "workspace:*",
    "@pinata/sdk": "^2.1.0",
    "@vanilla-extract/css": "1.14.2",
    "@vanilla-extract/recipes": "0.5.1",
    "classnames": "^2.3.1",
    "date-fns": "~2.30.0",
    "firebase": "^10.8.0",
    "firebase-functions": "^4.7.0",
    "framer-motion": "^11.0.5",
    "graphql": "~16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "http-proxy-middleware": "^2.0.6",
    "next": "14.2.2",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-aria-components": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-qrcode-logo": "^2.9.0",
    "react-swipeable-list": "^1.9.3",
    "swr": "^2.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "~4.2.0",
    "@graphql-codegen/typescript-react-apollo": "~4.3.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@kadena/fonts": "~0.0.1",
    "@rushstack/eslint-config": "~3.6.9",
    "@testing-library/react": "~14.0.0",
    "@types/gtag.js": "~0.0.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@vanilla-extract/next-plugin": "2.4.0",
    "@vanilla-extract/vite-plugin": "4.0.7",
    "@vanilla-extract/webpack-plugin": "2.3.7",
    "@vitest/coverage-v8": "^1.6.0",
    "cache-sh": "^1.2.1",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "dotenv": "~16.4.5",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "happy-dom": "^12.9.1",
    "mini-css-extract-plugin": "2.7.6",
    "next-router-mock": "^0.9.10",
    "prettier": "~3.2.5",
    "ts-node": "~10.9.2",
    "typescript": "5.4.5",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  }
}
