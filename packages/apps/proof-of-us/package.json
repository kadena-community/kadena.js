{
  "name": "@kadena/proof-of-us",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "build": "pnpm run generate:sdk && next build",
    "create-event": "ts-node -T ./src/scripts/createEvent.ts",
    "dev": "concurrently --kill-others \"npm:next:dev\" \"npm:generate:sdk:watch\"",
    "dev:save": "next dev  --experimental-https",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:src": "prettier . --cache --write",
    "generate:sdk": "cache-sh -i \"{../graph/generated-schema.graphql,src/**/*.graph.ts,src/__generated__/**}\" -- graphql-codegen --config codegen-sdk.yml",
    "generate:sdk:watch": "chokidar \"src/**/*.graph.ts\" -c \"cache-sh -i \"{../graph/generated-schema.graphql,src/**/*.graph.ts,src/__generated__/**}\" -- graphql-codegen --config codegen-sdk.yml\" --initial",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:src": "eslint src --ext .js,.ts,.jsx,.tsx,.mjs",
    "next:dev": "next dev",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apollo/client": "~3.8.8",
    "@google-analytics/data": "~3.2.2",
    "@graphql-yoga/apollo-link": "~3.0.3",
    "@kadena/client": "workspace:*",
    "@kadena/pactjs": "workspace:*",
    "@kadena/react-icons": "workspace:*",
    "@kadena/react-ui": "workspace:*",
    "@vanilla-extract/css": "1.14.0",
    "@vanilla-extract/recipes": "0.5.1",
    "classnames": "^2.3.1",
    "date-fns": "~2.30.0",
    "firebase": "^10.8.0",
    "framer-motion": "^11.0.5",
    "graphql": "~16.8.1",
    "next": "13.5.5",
    "next-themes": "^0.2.1",
    "nft.storage": "^7.1.1",
    "react": "^18.2.0",
    "react-aria-components": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-qrcode-logo": "^2.9.0",
    "swr": "^2.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript-operations": "~4.0.1",
    "@graphql-codegen/typescript-react-apollo": "~4.1.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@kadena/fonts": "~0.0.1",
    "@rushstack/eslint-config": "~3.3.0",
    "@testing-library/react": "~14.0.0",
    "@types/gtag.js": "~0.0.12",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@vanilla-extract/next-plugin": "2.3.2",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "@vanilla-extract/webpack-plugin": "2.3.1",
    "@vitest/coverage-v8": "^1.1.0",
    "cache-sh": "^1.2.1",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.2.2",
    "dotenv": "~16.0.3",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "happy-dom": "^12.9.1",
    "mini-css-extract-plugin": "2.7.6",
    "next-router-mock": "^0.9.10",
    "prettier": "~3.0.3",
    "ts-node": "~10.8.2",
    "typescript": "5.2.2",
    "vitest": "^1.1.0",
    "vitest-dom": "^0.1.1"
  }
}
