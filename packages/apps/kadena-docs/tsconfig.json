{
  "extends": "./node_modules/@kadena-dev/heft-rig/profiles/default/tsconfig-base.json",
  "compilerOptions": {
    "types": ["heft-jest", "node", "@types/gtag.js"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ES2020",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"]
}
