{
  "name": "@kadena/kadena-docs",
  "version": "0.1.0",
  "scripts": {
    "7d": "npx 7d pinecone-create-index --index kda-docs --environment asia-northeast1-gcp",
    "7d:ingest": "npx 7d ingest --files 'src/pages/docs/**/*.md'  --files 'src/pages/docs/**/*.mdx' --namespace kda-docs",
    "7d:query": "npx 7d",
    "_phase:build": "npm run build",
    "_phase:test": "npm run test",
    "build": "npm run build:next",
    "build:next": "npm run createtree && next build",
    "createtree": "node ./src/scripts/getdocstree.mjs",
    "cypress:ci": "NEXT_PUBLIC_APP_DEV=test start-server-and-test dev http://localhost:3000 cypress:run",
    "cypress:cilocal": "NEXT_PUBLIC_APP_DEV=test start-server-and-test dev http://localhost:3000 cypress:open",
    "cypress:open": "percy exec -- cypress open",
    "cypress:run": "percy exec -- cypress run",
    "dev": "next dev",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx --fix",
    "lint-staged": "lint-staged",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "echo 'No tests, until there is time to debug the build'"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@7-docs/edge": "~0.1.7",
    "@babel/plugin-syntax-import-assertions": "~7.20.0",
    "@google-analytics/data": "~3.2.2",
    "@kadena/react-components": "workspace:*",
    "@kadena/react-ui": "workspace:*",
    "@mailchimp/mailchimp_marketing": "~3.0.80",
    "@mdx-js/loader": "~2.3.0",
    "@mdx-js/react": "~2.3.0",
    "@next/mdx": "~13.3.4",
    "acorn": "~8.8.2",
    "date-fns": "~2.30.0",
    "js-yaml": "~4.1.0",
    "mdast-util-to-string": "~3.2.0",
    "next": "13.4.4",
    "next-themes": "~0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-pretty-code": "~0.9.5",
    "remark-frontmatter": "~4.0.1",
    "remark-gfm": "~3.0.1",
    "shiki": "~0.14.2"
  },
  "devDependencies": {
    "@7-docs/cli": "~0.1.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/eslint-plugin": "workspace:*",
    "@kadena-dev/heft-rig": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@kadena/types": "workspace:*",
    "@percy/cli": "~1.24.0",
    "@percy/cypress": "~3.1.2",
    "@rushstack/eslint-config": "~3.3.0",
    "@rushstack/heft": "~0.50.6",
    "@rushstack/heft-jest-plugin": "~0.5.12",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-mdx-gfm": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-webpack5": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@types/gtag.js": "~0.0.12",
    "@types/heft-jest": "~1.0.3",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "18.0.11",
    "@vanilla-extract/next-plugin": "^2.1.2",
    "cypress": "~12.12.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "13.0.6",
    "jest": "~29.5.0",
    "jest-standard-reporter": "~2.0.0",
    "lint-staged": "~13.2.0",
    "mdast-util-from-markdown": "~1.3.0",
    "mdast-util-frontmatter": "~1.0.1",
    "micromark-extension-frontmatter": "~1.1.0",
    "prettier": "^2.8.5",
    "sort-package-json": "~2.4.1",
    "start-server-and-test": "~2.0.0",
    "storybook": "^7.0.20",
    "storybook-addon-next-router": "~4.0.2",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "5.0.4"
  }
}
