{
  "name": "@kadena/kadena-docs",
  "version": "0.1.0",
  "scripts": {
    "_phase:build": "npm run build",
    "_phase:test": "npm run test",
    "build": "next build",
    "createtree": "node ./src/scripts/getdocstree.mjs",
    "cypress:ci": "NEXT_PUBLIC_APP_DEV=test start-server-and-test dev http://localhost:3000 cypress:run",
    "cypress:cilocal": "NEXT_PUBLIC_APP_DEV=test start-server-and-test dev http://localhost:3000 cypress:open",
    "cypress:open": "percy exec -- cypress open",
    "cypress:run": "percy exec -- cypress run",
    "dev": "next dev",
    "lint": "npx eslint ./src --ext .js,.ts,.jsx,.tsx --fix",
    "lint-staged": "lint-staged",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@kadena/react-components": "workspace:*",
    "@markdoc/markdoc": "~0.2.2",
    "@markdoc/next.js": "~0.2.2",
    "js-yaml": "~4.1.0",
    "next": "13.0.6",
    "next-themes": "~0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/eslint-plugin": "workspace:*",
    "@kadena-dev/heft-rig": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@kadena/types": "workspace:*",
    "@percy/cli": "~1.24.0",
    "@percy/cypress": "~3.1.2",
    "@rushstack/eslint-config": "~2.6.2",
    "@rushstack/heft": "~0.46.1",
    "@rushstack/heft-jest-plugin": "~0.3.16",
    "@storybook/addon-actions": "^7.0.0-rc.3",
    "@storybook/addon-essentials": "^7.0.0-rc.3",
    "@storybook/addon-interactions": "^7.0.0-rc.3",
    "@storybook/addon-links": "^7.0.0-rc.3",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.3",
    "@storybook/react": "^7.0.0-rc.3",
    "@storybook/react-webpack5": "^7.0.0-rc.3",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/heft-jest": "~1.0.3",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "18.0.11",
    "cypress": "~12.9.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.0.6",
    "jest": "~29.5.0",
    "jest-standard-reporter": "~2.0.0",
    "lint-staged": "~13.2.0",
    "mdast-util-from-markdown": "~1.3.0",
    "mdast-util-frontmatter": "~1.0.1",
    "micromark-extension-frontmatter": "~1.1.0",
    "prettier": "^2.8.5",
    "sort-package-json": "~2.4.1",
    "start-server-and-test": "~2.0.0",
    "storybook": "^7.0.0-rc.3",
    "storybook-addon-next-router": "~4.0.2",
    "storybook-dark-mode": "^2.1.1",
    "typescript": "^4.9.5"
  }
}
