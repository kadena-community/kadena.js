    "@kadena/pactjs-cli": "workspace:*",
    "@rushstack/eslint-config": "~3.3.0",
    "@rushstack/heft": "~0.50.6",
    "@types/debug": "~4.1.7",
    "@types/heft-jest": "~1.0.3",
    "@types/node": "^16.0.0",
    "@types/pg": "~8.10.2",
    "embedded-postgres": "~15.1.0-beta.6",
    "prettier": "~3.0.0",
    "prettier-plugin-packagejson": "^2.4.5",
    "ts-node": "~10.8.2"
