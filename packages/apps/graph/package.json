    "debug": "~4.3.4",
    "dotenv": "~16.0.3",
    "eslint": "^8.45.0",
    "graphql": "~16.8.0",
    "graphql-scalars": "~1.22.2",
    "graphql-yoga": "~4.0.4",
    "json-bigint-patch": "~0.0.8",
    "prisma": "^5.1.1"
  },
  "devDependencies": {
    "@kadena-dev/eslint-config": "workspace:*",
