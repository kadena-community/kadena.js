{
  "name": "@kadena/dev-wallet",
  "version": "0.0.5",
  "private": true,
  "description": "Kadena developer wallet",
  "type": "module",
  "scripts": {
    "prebuild": "npm run generate-coin-type",
    "build": "pnpm run prebuild && tsc && vite build",
    "build-dev": "vite build --mode development --watch",
    "dev": "vite",
    "generate-coin-type": "pactjs contract-generate --contract \"coin\" --api \"https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/14/pact\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@kadena/client": "workspace:*",
    "@kadena/client-utils": "workspace:*",
    "@kadena/cryptography-utils": "workspace:*",
    "@kadena/hd-wallet": "workspace:*",
    "@kadena/pactjs-generator": "workspace:*",
    "@kadena/pactjs": "workspace:*",
    "@kadena/react-ui": "workspace:*",
    "@scure/bip39": "^1.2.1",
    "@tauri-apps/api": "^1.4.0",
    "@vanilla-extract/css": "1.14.0",
    "chrome-types": "^0.1.248",
    "ed25519-keygen": "^0.4.8",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-dom": "^6.15.0",
    "swr": "^2.2.2",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@kadena/pactjs-cli": "workspace:*",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-static-copy": "^1.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.1.0"
  }
}