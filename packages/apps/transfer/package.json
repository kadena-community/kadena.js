{
  "name": "@kadena/transfer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run generate:icons && npm run generate:contracts && next build",
    "dev": "npm run generate:icons && next dev",
    "eject": "react-scripts eject",
    "format": "prettier locales src --write -u",
    "generate:icons": "npx @svgr/cli --typescript --out-dir src/resources/svg/generated -- src/resources/svg",
    "generate:coin:contract": "npx @kadena/pactjs-cli contract-generate --contract coin --api https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/1/pact --chain 1 --network testnet",
    "generate:contracts": "npm run generate:coin:contract && npm run generate:faucet:contract",
    "generate:faucet:contract": "npx @kadena/pactjs-cli contract-generate --contract user.coin-faucet --api https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/1/pact --chain 1 --network testnet",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "test": "jest --silent"
  },
  "dependencies": {
    "@kadena/chainweb-node-client": "workspace:*",
    "@kadena/client": "workspace:*",
    "@kadena/cryptography-utils": "workspace:*",
    "@kadena/pactjs": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@kadena/react-components": "workspace:*",
    "@radix-ui/colors": "~0.1.8",
    "@stitches/react": "1.3.1-1",
    "ace-builds": "~1.22.1",
    "jest-standard-reporter": "~2.0.0",
    "next": "13.4.4",
    "next-translate": "~2.0.6",
    "react": "^18.2.0",
    "react-ace": "~10.1.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@jest/reporters": "~29.5.0",
    "@jest/types": "~29.5.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/eslint-plugin": "workspace:*",
    "@kadena-dev/heft-rig": "workspace:*",
    "@kadena/types": "workspace:*",
    "@next/eslint-plugin-next": "^12.0.8",
    "@rushstack/eslint-config": "~3.3.0",
    "@rushstack/heft": "~0.50.6",
    "@rushstack/heft-jest-plugin": "~0.5.12",
    "@svgr/cli": "~8.0.1",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~14.0.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/heft-jest": "~1.0.3",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "~5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "13.0.6",
    "eslint-plugin-jest": "~27.2.1",
    "ignore-loader": "~0.1.2",
    "jest": "~29.5.0",
    "jest-environment-jsdom": "~29.5.0",
    "next-translate-plugin": "~2.0.5",
    "prettier": "~2.8.8",
    "typescript": "5.0.4"
  },
  "volta": {
    "node": "16.15.0"
  }
}
