    "@types/node": "^16.0.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "~5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vanilla-extract/css": "1.11.1",
    "debug": "~4.3.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.15.0",
