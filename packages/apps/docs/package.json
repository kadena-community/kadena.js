{
  "name": "@kadena/docs",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "7d:ingest": "npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx'  --files 'src/specs/**/*.json' --namespace kda-docs",
    "7d:ingest-algolia-prod": "pnpm run build:scripts && pnpm run algolia-prod",
    "7d:query": "npx 7d",
    "algolia-dev": "ALGOLIA_INDEX_NAME=docs_website_dev OPENAI_API_KEY=dummy-key121 npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx' --files 'src/specs/**/*.json' --db algolia --skip-embeddings",
    "algolia-prod": "ALGOLIA_INDEX_NAME=docs_website_prod OPENAI_API_KEY=dummy-key121 npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx'  --files 'src/specs/**/*.json' --db algolia --skip-embeddings",
    "algoliadev:scripts": "tsx ./src/scripts/algoliaIngestion.ts",
    "build": "pnpm run build:scripts && pnpm run build:next",
    "build:changelogs": "tsx ./src/scripts/importChangelogs/index.ts",
    "build:e2e": "pnpm run build:scripts && NODE_ENV=test pnpm run build:next",
    "build:next": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:scripts": "tsx ./src/scripts/build.ts",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 IGNOREREPO=true NODE_ENV=development pnpm run build:scripts && concurrently \"next dev\" \"pnpm run dev:docswatch\"",
    "dev:docswatch": "NODE_ENV=development tsx ./src/scripts/watchdocs.ts",
    "dev:full": "NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development pnpm run build:scripts && concurrently \"next dev\" \"pnpm run dev:docswatch\"",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:src": "prettier . --cache --write",
    "ingest-algolia-dev": "pnpm run build:scripts && pnpm run algoliadev:scripts",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts,.jsx,.tsx,.mjs",
    "prettier": "prettier",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest  --coverage",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@7-docs/edge": "^0.5.1",
    "@google-analytics/data": "~3.2.2",
    "@kadena/docs-tools": "workspace:*",
    "@kadena/fonts": "~0.0.1",
    "@kadena/react-icons": "workspace:*",
    "@kadena/react-ui": "workspace:*",
    "@mailchimp/mailchimp_marketing": "~3.0.80",
    "@mdx-js/loader": "~2.3.0",
    "@mdx-js/react": "~2.3.0",
    "@next/mdx": "~13.3.4",
    "@vanilla-extract/css": "1.14.2",
    "acorn": "~8.11.2",
    "algoliasearch": "^4.20.0",
    "classnames": "^2.3.1",
    "date-fns": "~2.30.0",
    "globby": "~13.2.2",
    "js-yaml": "~4.1.0",
    "mdast-util-to-markdown": "~1.5.0",
    "mdast-util-to-string": "~3.2.0",
    "mermaid": "^10.9.0",
    "mobx": "~6.9.0",
    "next": "14.2.2",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "~8.0.7",
    "react-tweet": "~3.1.1",
    "react-use": "^17.4.0",
    "redoc": "~2.0.0",
    "rehype-pretty-code": "~0.9.5",
    "rehype-raw": "^7.0.0",
    "remark-frontmatter": "~5.0.0",
    "remark-gfm": "~3.0.1",
    "sharp": "^0.33.1",
    "shiki": "~0.14.2",
    "styled-components": "~5.3.10"
  },
  "devDependencies": {
    "@7-docs/cli": "^0.5.1",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@percy/cli": "~1.24.0",
    "@rushstack/eslint-config": "~3.6.9",
    "@swc-node/register": "^1.8.0",
    "@swc/core": "~1.3.78",
    "@testing-library/react": "~14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "~14.5.1",
    "@types/gtag.js": "~0.0.12",
    "@types/js-yaml": "4.0.9",
    "@types/mailchimp__mailchimp_marketing": "~3.0.10",
    "@types/mdast": "^4.0.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/xml2js": "^0.4.14",
    "@vanilla-extract/next-plugin": "2.4.0",
    "@vanilla-extract/vite-plugin": "4.0.7",
    "@vanilla-extract/webpack-plugin": "2.3.7",
    "@vitest/coverage-v8": "^1.6.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "chromatic": "6.20.0",
    "concurrently": "^8.2.2",
    "dotenv": "~16.4.5",
    "eslint": "^8.45.0",
    "log-update": "^5.0.1",
    "markdown-link-extractor": "^3.1.0",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "~1.3.0",
    "mdast-util-frontmatter": "~1.0.1",
    "micromark-extension-frontmatter": "~1.1.0",
    "mini-css-extract-plugin": "2.7.6",
    "next-plugin-yaml": "^1.0.1",
    "next-router-mock": "^0.9.10",
    "octokit": "^3.2.1",
    "openapi-types": "~12.1.3",
    "prettier": "~3.2.5",
    "recursive-copy": "^2.0.14",
    "remark": "~14.0.3",
    "remark-cli": "~12.0.0",
    "remark-validate-links": "^13.0.0",
    "tsx": "^3.12.10",
    "typescript": "5.4.5",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1",
    "xml2js": "^0.6.2"
  }
}
