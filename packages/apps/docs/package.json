{
  "name": "@kadena/docs",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "7d": "npx 7d pinecone-create-index --index kda-docs --environment asia-northeast1-gcp",
    "7d:clear": "npx 7d pinecone-clear-namespace --namespace kda-docs",
    "7d:ingest": "npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx'  --files 'src/specs/**/*.json' --namespace kda-docs",
    "7d:ingest-algolia-dev": "pnpm run build:scripts && pnpm run algolia-dev",
    "7d:ingest-algolia-prod": "pnpm run build:scripts && pnpm run algolia-prod",
    "7d:query": "npx 7d",
    "algolia-dev": "ALGOLIA_INDEX_NAME=docs_website_dev OPENAI_API_KEY=dummy-key121 npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx' --files 'src/specs/**/*.json' --db algolia --skip-embeddings",
    "algolia-prod": "ALGOLIA_INDEX_NAME=docs_website_prod OPENAI_API_KEY=dummy-key121 npx 7d ingest --files 'src/pages/**/*.md' --files 'src/pages/**/*.mdx'  --files 'src/specs/**/*.json' --db algolia --skip-embeddings",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 pnpm run build:scripts && next build",
    "build:next": "pnpm run build:scripts && next build",
    "build:scripts": "tsx ./src/scripts/build.ts",
    "build:storybook": "pnpm run build:scripts && storybook build",
    "chromatic": "chromatic -b build:storybook",
    "dev": "pnpm run build:scripts &&  next dev",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:src": "prettier . --cache --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts,.jsx,.tsx,.mjs",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "echo 'No tests, until there is time to debug the build'"
  },
  "dependencies": {
    "@7-docs/edge": "^0.5.1",
    "@google-analytics/data": "~3.2.2",
    "@kadena/docs-tools": "workspace:*",
    "@kadena/fonts": "~0.0.1",
    "@kadena/react-ui": "workspace:*",
    "@mailchimp/mailchimp_marketing": "~3.0.80",
    "@mdx-js/loader": "~2.3.0",
    "@mdx-js/react": "~2.3.0",
    "@next/mdx": "~13.3.4",
    "@vanilla-extract/css": "1.14.0",
    "acorn": "~8.11.2",
    "algoliasearch": "^4.20.0",
    "classnames": "^2.3.1",
    "date-fns": "~2.30.0",
    "globby": "~13.2.2",
    "js-yaml": "~4.1.0",
    "mdast-util-to-markdown": "~1.5.0",
    "mdast-util-to-string": "~3.2.0",
    "mobx": "~6.9.0",
    "next": "13.5.5",
    "next-themes": "~0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "~8.0.7",
    "react-tweet": "~3.1.1",
    "react-use": "^17.4.0",
    "redoc": "~2.0.0",
    "rehype-pretty-code": "~0.9.5",
    "rehype-raw": "^7.0.0",
    "remark": "~14.0.3",
    "remark-frontmatter": "~5.0.0",
    "remark-gfm": "~3.0.1",
    "shiki": "~0.14.2",
    "styled-components": "~5.3.10"
  },
  "devDependencies": {
    "@7-docs/cli": "^0.5.1",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@percy/cli": "~1.24.0",
    "@rushstack/eslint-config": "~3.3.0",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-controls": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-mdx-gfm": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "@swc/core": "~1.3.78",
    "@types/gtag.js": "~0.0.12",
    "@types/js-yaml": "4.0.9",
    "@types/mailchimp__mailchimp_marketing": "~3.0.10",
    "@types/mdast": "^4.0.1",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "18.2.7",
    "@vanilla-extract/next-plugin": "2.3.2",
    "@vanilla-extract/webpack-plugin": "2.3.1",
    "chalk": "^5.2.0",
    "chromatic": "6.20.0",
    "dotenv": "~16.0.3",
    "eslint": "^8.45.0",
    "log-update": "^5.0.1",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "~1.3.0",
    "mdast-util-frontmatter": "~1.0.1",
    "micromark-extension-frontmatter": "~1.1.0",
    "mini-css-extract-plugin": "2.7.6",
    "openapi-types": "~12.1.3",
    "prettier": "~3.0.3",
    "recursive-copy": "^2.0.14",
    "start-server-and-test": "~2.0.0",
    "storybook": "^7.4.0",
    "storybook-addon-next-router": "~4.0.2",
    "storybook-addon-swc": "~1.2.0",
    "storybook-dark-mode": "^3.0.0",
    "tsx": "^3.12.10",
    "typescript": "5.2.2"
  }
}
