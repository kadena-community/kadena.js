{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mainnet/graph/(.*)",
      "dest": "/api/mainnet/graph"
    },
    {
      "src": "/api/testnet/graph/(.*)",
      "dest": "/api/testnet/graph"
    },
    {
      "src": "/api/test(.*)",
      "dest": "/api/test"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "release/graphproxy-v*": true
    }
  }
}
