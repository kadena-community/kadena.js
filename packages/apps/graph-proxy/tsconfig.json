{
  "extends": "./node_modules/@kadena-dev/shared-config/tsconfig-base.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./public",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/api/**/*.ts"],
  "exclude": ["node_modules", "public"]
}
