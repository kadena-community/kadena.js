{
  "extends": "./node_modules/@kadena-dev/shared-config/tsconfig-base.json",
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules", "dist"]
}
