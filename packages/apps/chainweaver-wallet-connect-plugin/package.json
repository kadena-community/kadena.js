{
  "name": "@kadena/chainweaver-wallet-connect-plugin",
  "version": "0.0.4",
  "private": true,
  "description": "Wallet Connect plugin for Kadena Chainweaver wallet",
  "type": "module",
  "main": "dist/index.es.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "pnpm run build && concurrently \"chokidar './src/**' -s -t 500  --command './node_modules/.bin/vite build' \"",
    "dev": "vite --force",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "test": "vitest run",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@kadena/chainweb-node-client": "workspace:*",
    "@kadena/client": "workspace:*",
    "@kadena/client-utils": "workspace:*",
    "@kadena/cryptography-utils": "workspace:*",
    "@kadena/hd-wallet": "workspace:*",
    "@kadena/kode-icons": "workspace:*",
    "@kadena/kode-ui": "workspace:*",
    "@kadena/pactjs": "workspace:*",
    "@kadena/pactjs-generator": "workspace:*",
    "@reown/walletkit": "^1.2.0",
    "@scure/bip39": "1.2.1",
    "@tauri-apps/api": "^1.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@vanilla-extract/css": "1.14.2",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/recipes": "0.5.1",
    "@walletconnect/core": "^2.18.0",
    "@walletconnect/utils": "^2.18.0",
    "asn1js": "^3.0.5",
    "cbor-web": "^9.0.2",
    "chrome-types": "^0.1.248",
    "classnames": "^2.3.1",
    "ed25519-keygen": "0.4.8",
    "elliptic": "^6.5.5",
    "js-yaml": "~4.1.0",
    "react": "^18.2.0",
    "react-aria": "^3.31.1",
    "react-console-emulator": "^5.0.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-qrcode-logo": "^2.9.0",
    "react-router-dom": "^6.26.2",
    "swr": "^2.2.2",
    "usehooks-ts": "^2.9.1",
    "ws": "^8.16.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/elliptic": "^6.4.18",
    "@types/js-yaml": "4.0.9",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vanilla-extract/esbuild-plugin": "^2.3.11",
    "@vanilla-extract/vite-plugin": "4.0.17",
    "@vitejs/plugin-react-swc": "^3.2.2",
    "@walletconnect/types": "~2.8.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "5.4.5",
    "vite": "^5.4.8",
    "vite-plugin-static-copy": "^1.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.0"
  },
  "chainweaver-plugin-manifest": {
    "name": "Wallet Connect",
    "description": "Wallet Connect plugin for Kadena Chainweaver wallet",
    "permissions": [
      "MSG:CONNECTION_REQUEST",
      "MSG:SIGN_REQUEST",
      "MSG:GET_STATUS",
      "MSG:GET_NETWORK_LIST",
      "MSG:GET_ACCOUNTS",
      "MODE:FOREGROUND",
      "MODE:BACKGROUND",
      "MODE:ON_START"
    ]
  }
}
