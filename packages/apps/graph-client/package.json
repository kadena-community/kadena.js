{
  "name": "@kadena/graph-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rushx postinstall && next build",
    "dev": "concurrently --kill-others \"npm:next:dev\" \"npm:start:graph\" \"sleep 3; rushx generate:sdk -w\"",
    "format": "npm run format:md && npm run format:pkg && npm run format:src",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:pkg": "sort-package-json",
    "format:src": "prettier config src --write -u",
    "generate:sdk": "graphql-codegen --config codegen-sdk.yml",
    "postinstall": "npm run generate:sdk",
    "lint": "next lint",
    "next:dev": "next dev",
    "start": "next start",
    "start:graph": "cd ../graph && rushx start",
    "test": "echo \"No tests yet.\""
  },
  "dependencies": {
    "@apollo/client": "~3.7.2",
    "@graphql-yoga/apollo-link": "~1.0.1",
    "@radix-ui/colors": "~0.1.8",
    "@radix-ui/react-icons": "~1.1.1",
    "@stitches/react": "1.3.1-1",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.30",
    "@types/react-dom": "18.0.11",
    "eslint": "^8.15.0",
    "eslint-config-next": "13.4.5",
    "graphql": "~16.6.0",
    "graphql-sse": "~1.3.2",
    "lodash.isequal": "~4.5.0",
    "next": "13.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "~2.8.5",
    "@graphql-codegen/typescript-graphql-request": "~4.5.8",
    "@graphql-codegen/typescript-operations": "~2.5.10",
    "@graphql-codegen/typescript-react-apollo": "~3.3.7",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/eslint-plugin": "workspace:*",
    "@kadena-dev/heft-rig": "workspace:*",
    "@kadena-dev/markdown": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@rushstack/eslint-config": "~3.3.0",
    "@rushstack/heft": "~0.50.6",
    "@types/heft-jest": "~1.0.3",
    "@types/lodash.isequal": "~4.5.6",
    "@types/node": "^16.0.0",
    "concurrently": "^7.2.2",
    "prettier": "~2.8.8",
    "sort-package-json": "~2.4.1",
    "ts-node": "~10.8.2"
  }
}
