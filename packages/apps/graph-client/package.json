{
  "name": "@kadena/graph-client",
  "version": "0.1.14",
  "private": true,
  "scripts": {
    "build": "pnpm run generate:sdk && next build",
    "dev": "concurrently --kill-others \"npm:next:dev\" \"sleep 3; pnpm run generate:sdk -w\"",
    "dev:all": "concurrently --kill-others \"npm:next:dev\" \"npm:start:graph\"",
    "dev:all:generate": "concurrently --kill-others \"npm:next:dev\" \"npm:start:graph:generate\" \"sleep 3; pnpm run generate:sdk -w\"",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:src": "prettier . --cache --write",
    "generate:sdk": "graphql-codegen --config codegen-sdk.yml",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src",
    "next:dev": "next dev",
    "start": "next start",
    "start:graph": "cd ../graph && pnpm run start",
    "start:graph:generate": "cd ../graph && pnpm run start:generate",
    "test": "echo \"No tests yet.\"",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "~3.8.8",
    "@graphql-yoga/apollo-link": "~3.0.3",
    "@kadena/react-ui": "workspace:*",
    "@parcel/watcher": "^2.1.0",
    "eslint": "^8.45.0",
    "graphql": "~16.8.1",
    "graphql-ws": "^5.15.0",
    "next": "13.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript-operations": "~4.0.1",
    "@graphql-codegen/typescript-react-apollo": "~4.1.0",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/markdown": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@rushstack/eslint-config": "~3.3.0",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vanilla-extract/css": "1.14.0",
    "@vanilla-extract/next-plugin": "2.3.2",
    "concurrently": "^7.2.2",
    "prettier": "~3.0.3",
    "typescript": "5.2.2"
  }
}
