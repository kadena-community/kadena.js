{
  "extends": "./node_modules/@kadena/heft-rig/profiles/default/tsconfig-base.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "ES2020",
    "allowJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
