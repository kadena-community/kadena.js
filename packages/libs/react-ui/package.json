{
  "name": "@kadena/react-ui",
  "version": "0.7.1",
  "description": "A react component library built on Kadena's Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadena-community/kadena.js.git",
    "directory": "packages/libs/react-ui"
  },
  "license": "ISC",
  "author": "KADENA",
  "contributors": [
    "Eileen Guo <eileen@kadena.io>",
    "Salama Ashoush <salama@kadena.io>",
    "Sam Ferrero <ferrero@kadena.io>"
  ],
  "sideEffects": [
    "*.css.*",
    "dist/global.*",
    "dist/side-effects/**",
    "src/entries/global.ts"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./global": {
      "types": {
        "import": "./dist/global.d.mts",
        "require": "./dist/global.d.ts"
      },
      "import": "./dist/global.mjs",
      "require": "./dist/global.cjs"
    },
    "./styles": {
      "types": {
        "import": "./dist/styles.d.mts",
        "require": "./dist/styles.d.ts"
      },
      "import": "./dist/styles.mjs",
      "require": "./dist/styles.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "global",
    "styles"
  ],
  "scripts": {
    "build": "crackle package --clean --fix",
    "build:storybook": "storybook build",
    "build:test": "tsc --noEmit & tsc -p ./tsconfig.cjs.json --noEmit",
    "chromatic": "chromatic -b build:storybook",
    "dev": "crackle dev --webpack",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:src": "prettier . --cache --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest",
    "tokens:sync": "design-sync sync",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@kadena/fonts": "~0.0.1",
    "@react-aria/focus": "^3.16.0",
    "@react-aria/utils": "^3.23.0",
    "@react-stately/utils": "^3.9.0",
    "@vanilla-extract/css": "1.14.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/recipes": "0.5.1",
    "@vanilla-extract/sprinkles": "1.6.1",
    "classnames": "^2.3.1",
    "css-loader": "~6.8.1",
    "focus-trap-react": "~10.2.1",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "react": "^18.2.0",
    "react-aria": "^3.31.1",
    "react-dom": "^18.2.0",
    "react-stately": "^3.29.1"
  },
  "devDependencies": {
    "@crackle/cli": "^0.13.0",
    "@design-sync/cli": "^0.11.1",
    "@design-sync/vanilla-extract-plugin": "^0.7.0",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/markdown": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@react-types/shared": "^3.22.0",
    "@rushstack/eslint-config": "~3.3.0",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-controls": "^7.4.0",
    "@storybook/addon-docs": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addons": "^7.4.0",
    "@storybook/csf": "~0.1.1",
    "@storybook/docs-mdx": "~0.1.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "@testing-library/react": "~14.0.0",
    "@testing-library/user-event": "~14.5.1",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.mapvalues": "^4.6.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "18.2.7",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "@vanilla-extract/webpack-plugin": "2.3.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "chromatic": "6.20.0",
    "copyfiles": "2.4.1",
    "csstype": "^3.1.2",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-storybook": "~0.6.13",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "~3.0.3",
    "prop-types": "^15.8.1",
    "rimraf": "~5.0.1",
    "storybook": "^7.4.0",
    "storybook-dark-mode": "^3.0.0",
    "tsc-alias": "~1.8.7",
    "typescript": "5.2.2",
    "vitest": "^1.1.0",
    "vitest-dom": "^0.1.1"
  }
}
