{
  "extends": "@kadena-dev/shared-config/tsconfig-base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "types": ["node"],
    "skipLibCheck": true,
    "module": "ESNext",
    "noEmit": true,
    "moduleResolution": "Bundler",
    "removeComments": true
  },
  "include": ["./src", "./scripts"],
  "exclude": ["node_modules"]
}
