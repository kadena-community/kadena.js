{
  "name": "@kadena/react-icons",
  "version": "0.0.1",
  "description": "Kadena's react icons library",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadena-community/kadena.js.git",
    "directory": "packages/libs/react-icons"
  },
  "license": "ISC",
  "author": "KADENA",
  "contributors": [
    "Salama Ashoush <salama@kadena.io>"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm generate:icons && unbuild",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:src": "prettier . --cache --write",
    "generate:icons": "tsx scripts/generateIcons.ts",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts,.tsx",
    "verify": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@kadena-dev/shared-config": "workspace:*",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "18.2.7",
    "giget": "1.2.1",
    "prettier": "~3.0.3",
    "react": "18.2.0",
    "scule": "^1.3.0",
    "tiny-readdir-glob": "^1.2.1",
    "tsx": "^4.7.1",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
