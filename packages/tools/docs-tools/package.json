{
  "name": "@kadena/docs-tools",
  "version": "0.2.1",
  "private": true,
  "description": "Helper utils to docs generation and adding extra context to markdown files",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadena-community/kadena.js.git",
    "directory": "packages/tools/docs-tools"
  },
  "license": "BSD-3-Clause",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run cleanup &&  pnpm run build:esm && pnpm run build:commonjs",
    "build:commonjs": "tsc -p ./tsconfig.cjs.json && tsc-alias -p ./tsconfig.cjs.json",
    "build:esm": "tsc && tsc-alias",
    "cleanup": "rimraf dist && rimraf lib",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:src": "prettier . --cache --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "acorn": "~8.11.2",
    "algoliasearch": "^4.20.0",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "~16.4.5",
    "fast-glob": "^3.3.1",
    "isomorphic-unfetch": "^4.0.2",
    "mdast-util-from-markdown": "~1.3.0",
    "mdast-util-frontmatter": "~1.0.1",
    "mdast-util-gfm": "3.0.0",
    "mdast-util-to-markdown": "~1.5.0",
    "mdast-util-to-string": "~3.2.0",
    "micromark-extension-frontmatter": "~1.1.0",
    "ora": "7.0.1",
    "remark": "^14.0.3",
    "remark-frontmatter": "~5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-inline-links": "^6.0.1",
    "remark-mdx": "^2.3.0",
    "unist-builder": "^4.0.0",
    "yaml": "~2.1.3"
  },
  "devDependencies": {
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@testing-library/react": "~14.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/unist": "^3.0.2",
    "@vitest/coverage-v8": "^1.6.0",
    "date-fns": "~2.30.0",
    "eslint": "^8.45.0",
    "js-yaml": "~4.1.0",
    "mdast": "^3.0.0",
    "prettier": "~3.2.5",
    "react": "^18.2.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rimraf": "~5.0.1",
    "tsc-alias": "~1.8.7",
    "typescript": "5.4.5",
    "unified": "10.1.2",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  }
}
