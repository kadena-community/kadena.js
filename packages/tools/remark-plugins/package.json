{
  "name": "@kadena-dev/markdown",
  "version": "1.0.0",
  "private": true,
  "description": "Kadena monorepo Remark plugins",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "bin": {
    "remark": "./node_modules/remark-cli/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "format": "pnpm run /^format:.*/",
    "format:md": "remark README.md -o --use ./lib/index.js",
    "format:src": "prettier . --cache --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:src": "eslint src --ext .ts --fix",
    "test": ""
  },
  "dependencies": {
    "mdast-builder": "~1.1.1",
    "mdast-comment-marker": "^2.1.2",
    "mdast-zone": "^5.1.1",
    "remark-cli": "~11.0.0",
    "remark-frontmatter": "~4.0.1",
    "remark-gfm": "~3.0.1",
    "remark-order-reference-links": "^0.0.3",
    "remark-parse": "^10.0.2",
    "remark-reference-links": "^6.0.1",
    "unified-prettier": "^1.0.1",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@kadena-dev/eslint-config": "workspace:*",
    "@rushstack/eslint-config": "~3.3.0",
    "@types/mdast": "^3.0.11",
    "@types/node": "^16.0.0",
    "eslint": "^8.45.0",
    "prettier": "~3.0.0",
    "prettier-plugin-packagejson": "^2.4.5",
    "typescript": "5.1.6",
    "unified": "^10.1.2",
    "vfile": "5.3.7"
  }
}
