{
  "extends": "@rushstack/heft-jest-plugin/includes/jest-shared.config.json",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "modulePathIgnorePatterns": ["/src/integration-tests"],
  "coveragePathIgnorePatterns": ["/node_modules/", "/tests/", "/integration-tests/"]
}
