{
  "name": "@kadena-dev/eslint-config",
  "version": "0.0.10",
  "description": "Kadena monorepo eslint-config",
  "license": "MIT",
  "files": [
    "mixins",
    "profile"
  ],
  "scripts": {
    "build": "",
    "format": "pnpm run /^format:.*/",
    "format:src": "prettier mixins profile --write",
    "lint": "eslint ./mixins ./profile --ext .js,.ts --fix",
    "test": ""
  },
  "dependencies": {
    "@kadena-dev/eslint-plugin": "workspace:*",
    "@next/eslint-plugin-next": "^12.0.8",
    "@rushstack/eslint-config": "~3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "13.4.5",
    "eslint-config-prettier": "~8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsx-a11y": "~6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "~7.31.11",
    "eslint-plugin-simple-import-sort": "~7.0.0",
    "prettier": "~3.0.0",
    "prettier-eslint": "~12.0.0",
    "eslint-plugin-json-files": "~3.0.0"
  },
  "devDependencies": {
    "typescript": "5.1.6"
  }
}
